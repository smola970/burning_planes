
<%= form_tag '/login' do %>

<p>
  <%= label_tag :email %>
  <%= text_field_tag :name %>
</p>

<p>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
</p>

<button class="btn btn-primary">Login</button>
</form>
<br>

<form action="/sign_up">
<button class="btn btn-primary">Sign Up</button></form>
<% end %>
